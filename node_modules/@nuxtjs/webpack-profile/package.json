{
  "name": "@nuxtjs/webpack-profile",
  "version": "0.1.0",
  "description": "Nuxt module to display webpack build progress steps with core ProgressPlugin",
  "license": "MIT",
  "contributors": [
    {
      "name": "pooya parsa <pooya@pi0.ir>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/nuxt-community/webpack-profile-module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib",
      "test"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "nuxt-module-builder": "latest"
  }
}
