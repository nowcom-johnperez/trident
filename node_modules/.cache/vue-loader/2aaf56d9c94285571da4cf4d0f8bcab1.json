    horizontal: {\n      type:    Boolean,\n      default: false\n    }\n  },\n\n  computed: {\n    ready() {\n      // Ensure we never show more ready machines than desired\n      // If w can address in backedn, we could revert this in the future\n      const ready = this.row?.ready || 0;\n      const desired = this.row?.desired || 0;\n\n      return ready > desired ? desired : ready;\n    }\n  },\n};\n</script>\n\n<template>\n  <v-popover\n    class=\"text-center hand machine-summary-graph\"\n    placement=\"top\"\n    :open-group=\"row.id\"\n    trigger=\"click\"\n    offset=\"1\"\n  >\n    <template #popover>\n      <table class=\"fixed\">\n        <tbody>\n          <tr\n            v-for=\"obj in row.stateParts\"\n            :key=\"obj.label\"\n          >\n            <td\n              class=\"text-left pr-20\"\n              :class=\"{[obj.textColor]: true}\"\n            >\n              {{ obj.label }}\n            </td>\n            <td class=\"text-right\">\n              {{ obj.value }}\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </template>\n\n    <div\n      class=\"content\"\n      :class=\"{ horizontal }\"\n    >\n      <ProgressBarMulti\n        v-if=\"row.stateParts\"\n        :values=\"row.stateParts\"\n        class=\"progress-bar\"\n      />\n      <span\n        v-if=\"row.desired === ready\"\n        class=\"count\"\n      >{{ ready }}</span>\n      <span\n        v-else\n        class=\"count\"\n      >{{ ready }} of {{ row.desired }}</span>\n    </div>\n  </v-popover>\n</template>\n\n<style lang=\"scss\" scoped>\n  .machine-summary-graph {\n    display: flex;\n    align-items: center;\n\n    .content {\n      .progress-bar {\n        margin-bottom: 5px;\n      }\n\n      &.horizontal {\n        // When horizontal, put the number before the graph\n        align-items: center;\n        direction: rtl;\n        display: flex;\n\n        > * {\n          direction: ltr;\n        }\n\n        .progress-bar {\n          margin-bottom: 0;\n          margin-left: 10px;\n        }\n      }\n    }\n  }\n</style>\n"]}]}